generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MataPelajaran {
  id          Int      @id @default(autoincrement())
  kode        String   @unique
  nama        String
  deskripsi   String?
  sks         Int      @default(2)
  semester    Int
  status      Boolean  @default(true)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
